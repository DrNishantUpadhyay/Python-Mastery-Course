# Working with Lists and Palindromes
list1 = [1, 2, 1]
copy_list1 = list1.copy()
copy_list1.reverse()

if(copy_list1 == list1):
    print("The list is a Palindrome")

# Dictionary of word meanings
meanings = {
    "cat": "a small animal",
    "table": ["a piece of furniture", "list of facts"]
}
print(meanings)

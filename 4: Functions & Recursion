# Function to calculate factorial
def cal_fact(n):
    fact = 1
    for i in range(1, n + 1):
        fact *= i
    return fact

# Recursive function to sum first N numbers
def calc_sum(n):
    if(n == 0):
        return 0
    return calc_sum(n - 1) + n

print("Factorial of 5:", cal_fact(5))
print("Sum of first 10 numbers:", calc_sum(10))
